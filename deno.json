{
  "tasks": {
    "start": "deno run --unstable -A --watch=static/,routes/ dev.ts",
    "test": "KV_PATH=\":memory:\" deno test --unstable --allow-read=. --allow-env",
    "update": "deno cache --reload --lock-write main.ts"
  },
